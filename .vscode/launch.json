    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch my cool app",
            "url": "http://localhost:9099"
          },
          {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to browser",
            "port": 9222
          }
    ]
}